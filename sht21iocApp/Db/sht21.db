record(ai, "$(user):C0:humidity:cur")
{
    field (DESC, "humidity")
 	field (INP, "#C1 S17 @rh")
    field (DTYP, "sht21")
    field (SCAN, "I/O Intr")
    field (EGU, "%")
    field (PREC, "1")
    field (HIGH, 100)
    field (LOW, 0)
}

record(ai, "$(user):C0:temperature:cur")
{
    field (DESC, "temperature")
 	field (INP, "#C1 S17 @temp")
    field (DTYP, "sht21")
    field (SCAN, "I/O Intr")
    field (EGU, "degC")
    field (PREC, "1")
    field (HIGH, 80)
    field (LOW, -40)
}

